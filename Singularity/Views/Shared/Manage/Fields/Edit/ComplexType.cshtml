@* Generator: MvcView *@
@using Singularity.Extensions;

@{

}

@model IViewField

@if (Model.PropertyData != null)
    {
    IEnumerable<ModelMetadata> ModelMetadata = Model.Meta.ModelType.Meta()?.Properties
        ?? new List<ModelMetadata>();

    foreach (var SubMeta in ModelMetadata)
        {
        var Field = new ViewField(ViewContext, SubMeta.ModelType, $"{Model.PropertyName}.{SubMeta.PropertyName}", (IModel)Model.PropertyData, Model.ViewTypes);

        @Html.ViewField(Field)
        }
    }