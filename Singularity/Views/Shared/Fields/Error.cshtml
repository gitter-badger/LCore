@* Generator: MvcView *@

@{

}

@model Exception

<span class="field-error error">

    @if (ContextProviderFactory.GetCurrent().CurrentUser(Session) != null &&
                ContextProviderFactory.GetCurrent().CurrentUser(Session).IsAdmin == true)
        {
        <a title="@Model.ToString()">Error</a>
        }
    else
        {
        <a>Error</a>
        }
</span>