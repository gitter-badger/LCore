<body singularity class="@(ViewBag.ContextProvider != null ? StringExt.ToUrlSlug(ViewBag.ContextProvider.GetContext(Session).ContextName) : "")">

    <script type="text/javascript">

        $(document).ready(function () {

        })
    </script>
    <header>
        @*Html.Partial("_Menu") *@

        @if (TempData.Peek(ControllerHelper.StatusMessage) != null)
        {
            String Message = (String)TempData[ControllerHelper.StatusMessage];
            String Icon = (String)(TempData[ControllerHelper.StatusMessageIcon] ?? "");

            <div class="status-message close-dialog">
                @if (!String.IsNullOrEmpty(Icon))
                {
                    <glyph>@Icon</glyph>
                }
                @Message
            </div>
        }

        <div class="key-bind-tip" style="display:none">
            <div class="border-bottom">
                <glyph>&#xe047;</glyph>
                Key Shortcuts
            </div>
            <span id="key-bind-page-tip">
            </span>
        </div>
    </header>
    <div id="body" data-role="page">

        @RenderSection("featured", required: false)

        <section class="content-wrapper main-content clear-fix">
            @RenderBody()
        </section>
    </div>
    <footer>
        <div class="content-wrapper">
            <div class="float-left">
                <p>&copy; @DateTime.Now.Year</p>
            </div>
        </div>
    </footer>

    <script src="/Scripts/jquery-mobile/jquery-1.11.1.min.js"></script>

    @RenderSection("scripts", required: false)
